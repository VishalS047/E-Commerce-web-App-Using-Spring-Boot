package com.shopme.frontend;

import org.springframework.boot.SpringApplication;
import org.springframework.boot.autoconfigure.SpringBootApplication;
import org.springframework.boot.autoconfigure.domain.EntityScan;
import org.springframework.context.annotation.ComponentScan;

@SpringBootApplication
@EntityScan({"com.shopme.controller", "com.shopme.repository", "com.shopme.security", "com.shopme.repotest"})
@ComponentScan({"com.shopme.controller", "com.shopme.repository", "com.shopme.security", "com.shopme.repotest"})
public class ShopmeFrontEndApplication {

	public static void main(String[] args) {
		SpringApplication.run(ShopmeFrontEndApplication.class, args);
	}

}
